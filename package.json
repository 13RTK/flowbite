    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.3.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
